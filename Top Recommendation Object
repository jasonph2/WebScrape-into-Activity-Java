#Basic Java Object 

package edu.illinois.cs.cs124.ay2022.mp.models;

public class TopRecommendation {
  private final String image;
  private final String name;

  public TopRecommendation(final String setImage, final String setName) {
    this.image = setImage;
    this.name = setName;
  }
  #this Url is later switched to an image using Picasso implementation
  public String getImage() {
    return image;
  }
  public String getNameOfRecommendation() {
    return name;
  }
}
